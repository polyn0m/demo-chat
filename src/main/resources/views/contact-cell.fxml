<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="cellContainer" spacing="4.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="titleContainer" alignment="CENTER_LEFT" spacing="8.0" styleClass="title">
         <children>
            <Label fx:id="titleLabel" maxWidth="1.7976931348623157E308" styleClass="title-text" text="Фамилия Имя1" HBox.hgrow="ALWAYS" />
            <Label fx:id="titleCounterLabel" styleClass="title-unread-counter" text="10">
               <padding>
                  <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
               </padding>
            </Label>
         </children>
      </HBox>
      <Label fx:id="subTitleLabel" styleClass="sub-title" text="Сообщ..." />
   </children>
   <padding>
      <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
   </padding>
</VBox>
